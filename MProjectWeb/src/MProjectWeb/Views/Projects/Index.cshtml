@{
    //cambio exitoso de contraseña
    try
    {
        string cadPass = Context.Session.GetString("estPass");
        if (cadPass.Length > 0)
        {
            bool stR = Convert.ToBoolean(cadPass);
            if (stR)
            {
                <div>
                    <div class="alert alert-success fade in center-block" style="position:relative;width:100%">
                        <a  class="close" data-dismiss="alert">&times;</a>
                        Se ha actualizado con exito tu contraseña.
                    </div>
                </div>
            }

            Context.Session.SetString("estPass", "");
        }
    }
    catch { }
    try
    {
        string cadImg = Context.Session.GetString("estImg");
        if (cadImg.Length > 0)
        {
